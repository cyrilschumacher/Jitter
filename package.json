    "electron-packager": "^5.2.1",
    "electron-prebuilt": "^0.37.0",
    "gulp": "*",
    "gulp-jade": "*",
    "gulp-sass": "*",
    "gulp-tslint": "*",
    "gulp-typescript": "*",
    "mocha": "^2.4.5",
    "tslint": "*",
    "typescript": "*"
  },
  "dependencies": {
    "classnames": "^2.2.3",
    "i18next": "^2.3.4",
    "i18next-browser-languagedetector": "0.2.2",
    "i18next-xhr-backend": "^0.5.3",
    "lodash": "^4.6.1",
    "react": "^0.14.7",
