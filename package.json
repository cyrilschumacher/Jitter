{
  "name": "jitter",
  "version": "0.0.1",
  "description": "Edit your language files in JSON format with ease!",
  "repository": {
    "type": "git",
    "url": "https://github.com/cyrilschumacher/jitter.git"
  },
  "scripts": {
    "release": "cd dist && npm install --production && electron-packager . Jitter --platform=all --arch=all --version=0.37.0 --out ../release",
    "start": "electron dist/index.js",
    "test": "mocha ./test/**/*.js"
  },
  "keywords": [
